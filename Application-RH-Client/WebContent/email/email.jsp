<div class="compose well">
	<form class="compose form-horizontal" action="sendEmail" method="post"
		enctype="multipart/form-data">
		<div class="page-header">
			<h4>Compose Email</h4>
			<div class="actions">
				<button type="submit" id="send" class="btn btn-danger marginR10">
					<strong>SEND</strong>
				</button>
				<button id="save" class="btn marginR10">Save</button>
				<button id="discard" class="btn marginR10">Discard</button>
			</div>
		</div>

		<input type="hidden" name="login"
			value="<s:property value='#session.user.getEmail()'/>" /> <input
			type="hidden" name="password"
			value="<s:property value='#session.user.getPasswordEmail()'/>" />

		<div class="form-row row-fluid">
			<div class="span12">
				<div class="row-fluid">
					<label class="form-label span2" for="to">To:</label> <input
						class="span10" id="to" name="recipient" type="text" />
						<span class="help-block blue span8">Plusieurs separer par virgule</span>
				</div>
			</div>
		</div>

		<div class="form-row row-fluid">
			<div class="span12">
				<div class="row-fluid">
					<label class="form-label span2" for="subject">Subject:</label>
					<div>
						<input class="span10" id="subject" name="subject" type="text" />
					</div>
				</div>
			</div>
		</div>
		<div class="form-row row-fluid">
			<div class="span12">
				<div class="row-fluid">
					<label class="form-label span2" for="subject">Fichier :</label>
					<div>
						<input class="span10" id="subject" name="fileUpload" type="file" />
					</div>
				</div>
			</div>
		</div>

		<div class="form-row row-fluid">
			<div class="span12">
				<div class="row-fluid">
					<textarea class="span12 myEmail" id="myEdit" rows="8"
						name="message"></textarea>
				</div>
			</div>
		</div>
	</form>

</div>
